<h2>Your transaction history</h2>
<% @transactions.each do |transaction| %>
  <%= cl_image_tag transaction.pokemon.photo.key, height: 450, width: 400, crop: :fill %>
  <p><%= transaction.pokemon.name %></p>
  <p><%= transaction.pokemon.id %></p>
  <p>Type: <%= transaction.pokemon.type_pokemon %></p>
  <p>Level: <%= transaction.pokemon.level %></p>
  <p><%= transaction.pokemon.description %></p>
  <p>From: <%= transaction.start_date %></p>
  <p>Until: <%= transaction.end_date %></p>

<% end %>